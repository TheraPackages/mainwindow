{
  "version": "0.1.3",
  "main": "${workspaceRoot}/main.vue",
  "launch": {
    "appid": "com.taobao.preview",
    "path": "${mainNodeModulesPath}/weex-run/resources/Preview.app",
    "pathAndroid": "${mainNodeModulesPath}/weex-run/resources/Preview.apk",
    "appidAndroid": "com.alibaba.falcon/com.alibaba.falcon.activity.MainActivity"
  },
  "type": "weex",
  "notifyOnSync": false,
  "transformPath":"${workspaceRoot}/build",
  "build": {
    "apply": "debug",
    "types": {
      "debug": {
        "minify": false,
        "web": false
      },
      "release": {
        "minify": true,
        "web": false
      }
    }
  },
  "mock": [
    {
      "path": "data.data.cmAboveResult.modules",
      "api": "mtop.tmall.search.searchproduct",
      "file": "${workspaceRoot}/mock.json"
    }
  ]
}
